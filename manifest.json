{
    "manifest_version": 2,
    "name": "Hello World Extension",
    "version": "1.0",
    "description": "Exibe 'Hello, World!' em uma janela pop-up",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  